{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "mongodb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prefer-destructuring": "off",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
    }
}
